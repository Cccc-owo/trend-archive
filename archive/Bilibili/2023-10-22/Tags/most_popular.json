ata": {"tag_id": 7782, "tag_name": "三国演义", "cover": "http://i0.hdslb.com/bfs/tag/75fd325ef255b04d4209c3b57c0722560643aac5.jpg", "head_cover": "", "content": "三国电视剧和电视剧衍生视频\r\n根据中国古典文学四大名著之一的《三国演义》改编电视剧和电视剧衍生，剧演述的是三国时代的故事，是魏、蜀、吴的兴亡史，起于桃园结义，终于三国归晋。着重表现的是从东汉末年到西晋初年将近一个世纪中多个政治集团间错综复杂、紧张尖锐的斗争，这种斗争发展成为连接不断的对政治权力的争夺和军事冲突，给人们描绘出三国时代政治风云的彩色画卷，展示出一幅幅波澜壮阔、惊心动魄的战斗风云图。\r\n", "short_content": "", "type": 3, "state": 0, "ctime": 1436866637, "count": {"view": 0, "use": 259286, "atten": 14321}, "is_atten": 0, "likes": 0, "hates": 0, "attribute": 0, "liked": 0, "hated": 0, "extra_attr": 0}, "day_count": 1}, "80600": {"data": {"tag_id": 80600, "tag_name": "刘备", "cover": "", "head_cover": "", "content": "", "short_content": "", "type": 3, "state": 0, "ctime": 1436866637, "count": {"view": 0, "use": 134399, "atten": 2094}, "is_atten": 0, "likes": 0, "hates": 0, "attribute": 0, "liked": 0, "hated": 0, "extra_attr": 0}, "day_count": 1}, "17447260": {"data": {"tag_id": 17447260, "tag_name": "萝卜刀", "cover": "", "head_cover": "", "content": "", "short_content": "", "type": 1, "state": 0, "ctime": 1605853298, "count": {"view": 0, "use": 4979, "atten": 1}, "is_atten": 0, "likes": 0, "hates": 0, "attribute": 0, "liked": 0, "hated": 0, "extra_attr": 0}, "day_count": 1}, "153818": {"data": {"tag_id": 153818, "tag_name": "华佗", "cover": "", "head_cover": "", "content": "", "short_content": "", "type": 0, "state": 0, "ctime": 1436866637, "count": {"view": 0, "use": 5878, "atten": 6}, "is_atten": 0, "likes": 0, "hates": 0, "attribute": 0, "liked": 0, "hated": 0, "extra_attr": 0}, "day_count": 1}, "7172": {"data": {"tag_id": 7172, "tag_name": "吕布", "cover": "", "head_cover": "", "content": "", "short_content": "", "type": 0, "state": 0, "ctime": 1436866637, "count": {"view": 0, "use": 157196, "atten": 1987}, "is_atten": 0, "likes": 0, "hates": 0, "attribute": 0, "liked": 0, "hated": 0, "extra_attr": 0}, "day_count": 1}, "9683": {"data": {"tag_id": 9683, "tag_name": "关羽", "cover": "", "head_cover": "",