[{"aid": 115322975685699, "videos": 1, "tid": 17, "tname": "单机游戏", "copyright": 1, "pic": "http://i1.hdslb.com/bfs/archive/6f88e168443f12ed139ea4c82899bc4277ca0800.jpg", "title": "霍格沃兹的金丹期交换生！", "pubdate": 1759720800, "ctime": 1759689038, "desc": "小时候看哈利波特看疯了\n每本都读过几十遍，倒背如流\n妈妈觉得我只看哈利波特，很愁\n令我暑假每天在图书馆看一本书\n我把图书馆的哈利波特周边同人书看光了\n\n其中有一本比较喜欢的，到现在都记得名字\n《霍格沃兹的中国留学生》\n不是文笔有多好\n是中国人的爽文\n反正就记得中式魔法很牛逼，干翻伏地魔的头等功\n\n霍格沃兹之遗出了几年了\n还记得刚出时激动地打开玩了几个小时失望地离去\n但我想，既然哈利波特是我童年非常重要的一部分\n况且短期内，应该也不会见到比遗产更好的哈利波特世界观游戏了\n还是做吧(✿◡‿◡)", "state": 0, "duration": 672, "mission_id": 4049978, "rights": {"bp": 0, "elec": 0, "download": 0, "movie": 0, "pay": 0, "hd5": 0, "no_reprint": 1, "autoplay": 1, "ugc_pay": 0, "is_cooperation": 0, "ugc_pay_preview": 0, "no_background": 0, "arc_pay": 0, "pay_free_watch": 0}, "owner": {"mid": 1232542331, "name": "伶舟梨", "face": "https://i2.hdslb.com/bfs/face/9a475f037975d5a3b6d16d56c32c0c94fe2f8ee6.jpg"}, "stat": {"aid": 115322975685699, "view": 820844, "danmaku": 1009, "reply": 1281, "favorite": 16423, "coin": 12666, "share": 2277, "now_rank": 0, "his_rank": 0, "like": 65301, "dislike": 0, "vt": 0, "vv": 820844, "fav_g": 0, "like_g": 1431}, "dynamic": "我，就是霍格沃兹的中国留学生！", "cid": 32870629980, "dimension": {"width": 3840, "height": 2160, "rotate": 0}, "short_link_v2": "https://b23.tv/BV1hSxuzcEk5", "first_frame": "http://i2.hdslb.com/bfs/storyff/_00001b4jo0rzo8kx62mc5mo4ezc3cyb_firsti.jpg", "pub_location": "上海", "cover43": "http://i2.hdslb.com/bfs/archive/881cbec3bd746c7821c0d070842ae1cbf1fbda4f.jpg", "tidv2": 2066, "tnamev2": "单机主机类游戏", "pid_v2": 1008, "pid_name_v2": "游戏", "bvid": "BV1hSxuzcEk5", "season_type": 0, "is_ogv": false, "ogv_info": null, "enable_vt": 0, "ai_rcmd": null, "rcmd_reason": {"content": "6万点赞", "corner_mark": 0}}, {"aid": 115322002606578, "videos": 1, "tid": 219, "tname": "汪星人", "copyright": 1, "pic": "http://i1.hdslb.com/bfs/archive/dbf7ae0c2dcc9769ce925dc0f73e87f8e11256e9.jpg", "title": "温驯", "pubdate": 1759674364, "ctime": 1759674365, "desc": 